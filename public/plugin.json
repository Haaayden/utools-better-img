{
  "pluginName": "Better Img",
  "description": "给图片添加阴影",
  "main": "index.html",
  "version": "1.0.0",
  "author": "陈十二",
  "preload": "preload.js",
  "logo": "app-logo.png",
  "features": [
    {
      "code": "default",
      "explain": "为图片添加阴影",
      "cmds": [
        "图片", "阴影", "加壳", "better", "img",
        {
          "type": "img",
          "label": "Better Img 给图片添加阴影"
        },
        {
          "type": "files",
          "label": "Better Img 给图片添加阴影",
          "fileType": "file",
          "match": "/\\.(?:png|jpg|jpeg)$/i"
        }
      ]
    }
  ]
}
